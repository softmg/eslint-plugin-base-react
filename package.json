{
  "name": "@softmg/eslint-plugin-base-react",
  "version": "0.0.1-0",
  "description": "A comprehensive linting solution that sweeps your code clean",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Anton",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist",
    "README.md",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://git@github.com:softmg/eslint-plugin-base-react.git"
  },
  "scripts": {
    "lint": "npm-run-all \"lint:*\"",
    "lint:eslint-docs": "npm-run-all \"update:eslint-docs -- --check\"",
    "lint:js": "eslint .",
    "test": "mocha tests --recursive",
    "update:eslint-docs": "eslint-doc-generator",
    "build": "rm -rf ./dist && mkdir ./dist && cp -r ./lib/* ./dist",
    "pack": "npm pack --pack-destination=./dist"
  },
